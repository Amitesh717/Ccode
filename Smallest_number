//Write a program to print the smallest number
#include <stdio.h>

int main()
{int a,b,c;
    printf("Enter three numbers : ");
    scanf("%d %d %d",&a,&b,&c);
if(a<b)
    if(b<c)
        printf("smallest is %d",a);
    else if(c<a)
        printf("smallest is %d",c);
    else if(b>c)
        printf("smallest is %d",a); 
if(a<c)
    if(b<a)
        printf("smallest is %d",b);
if(a>b)
    if(b>c)
        printf("smallest is %d",c);
    else
        printf("smallest is %d",b);
    return 0;
}
