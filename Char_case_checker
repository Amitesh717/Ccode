//write a program to checking character is Uppercase or lower case or Not a character
#include <stdio.h>

int main()
{char a;
    printf("Enter char:");
    scanf("%c",&a);
    if((a>='A')&&(a<='Z'))
    printf("Upper case");
    else if((a>='a') && (a<='z'))
    printf("Lower case");
    else
    printf("Not a character");

    return 0;
}
